worker_processes  1;

events {
    worker_connections  1024;
	}

http {
    include       mime.types;
    default_type  application/octet-stream;
    sendfile        on;
    keepalive_timeout  65;

upstream app {
   <%= @server_list -%>
}

server {
    listen 80;
    server_name <%= @nginx_server -%>;

    location / {
        proxy_pass http://app;
    }
}
}
